{
    "fecha": {
        "prefix": "fecha",
        "body": [
            "${CURRENT_DATE}/${CURRENT_MONTH}/${CURRENT_YEAR}"
        ],
        "description": "Inserta la fecha en formato día/mes/año"
    },
    "Lista": {
        "prefix": "lista",
        "body": [
            "- **${1}**: ${0}"
        ],
        "description": "Insertar un elemento de lista en negrita"
    },
    "Bloque de código Python": {
        "prefix": "python",
        "body": [
            "```python",
            "${0}",
            "```"
        ],
        "description": "Bloque de código en Python"
    },
    "Bloque de código Bash": {
        "prefix": "bash",
        "body": [
            "```bash",
            "${0}",
            "```"
        ],
        "description": "Bloque de código en Bash"
    },
    "Bloque de código Java": {
        "prefix": "java",
        "body": [
            "```java",
            "${0}",
            "```"
        ],
        "description": "Bloque de código en Java"
    },
    "Bloque de código JavaScript": {
        "prefix": "javascript",
        "body": [
            "```javascript",
            "${0}",
            "```"
        ],
        "description": "Bloque de código en JavaScript"
    },
    "Bloque de código HTML": {
        "prefix": "html",
        "body": [
            "```html",
            "${0}",
            "```"
        ],
