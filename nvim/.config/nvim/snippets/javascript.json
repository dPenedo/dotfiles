    },
    "NOTE Comment": {
        "scope": [
            "javascript",
            "javascriptreact",
            "typescript",
            "typescriptreact"
        ],
        "prefix": "note",
        "body": [
            "// NOTE: ${1}"
        ],
        "description": "Insert a NOTE comment"
    }
}
