{
    //Window
    "window.zoomLevel": 1,
    "window.density.editorTabHeight": "compact",
    //Editor
    // "hide-tabs.toggleTabs": true,
    "editor.tabSize": 4,
    "editor.lineHeight": 17,
    "editor.fontLigatures": true,
    "editor.fontFamily": "'FiraCode Nerd Font', 'JetBrainsMono Nerd Font', 'monospace', monospace",
    "editor.wordWrap": "bounded",
    "editor.wrappingIndent": "same",
    "editor.lineNumbers": "relative",
    "editor.wordWrapColumn": 100,
    "editor.folding": false,
    "editor.insertSpaces": true,
    "editor.smoothScrolling": false,
    "editor.minimap.enabled": false,
    "editor.detectIndentation": true,
    "editor.suggestSelection": "first",
    "editor.scrollBeyondLastLine": false,
    "editor.renderControlCharacters": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.quickSuggestionsDelay": 0,
    "editor.cursorBlinking": "solid",
    "editor.glyphMargin": true,
    "editor.bracketPairColorization.enabled": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.hideCursorInOverviewRuler": true,
    "editor.unicodeHighlight.allowedCharacters": {
        "а": true,
        "с": true,
        "Т": true,
        "б": true,
        "е": true
    },
    //Editor settings for formatting
    "editor.formatOnSave": true,
    //Workbench
    "catppuccin.accentColor": "peach",
    "catppuccin.colorOverrides": {
        "mocha": {
            "pink": "#F5C5C2",
            "base": "#161617",
            "mauve": "#A8A6F7"
        }
    },
    "workbench.colorCustomizations": {
        "[Kanagawa]": {
            "editorBracketMatch.background": "#363646",
            "editorBracketMatch.border": "#7e9cd8",
            // "editor.background": "#161617"
            "terminal.background": "#1b1b22",
            "editorLineNumber.foreground": "#5E6982",
            "editorLineNumber.activeForeground": "#ED9338",
            "editor.background": "#161617",
            "editorCursor.background": "#161617",
        },
    },
    "editor.semanticTokenColorCustomizations": {
        "[Kanagawa Black]": {
            "comments": "#8A8980",
            "background": "#161617"
        },
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "comment.block",
                    "comment.block.documentation",
                    "comment.line",
                    "comment.line.double-slash",
                    "punctuation.definition.comment",
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "editor.tokenColorCustomizations": {
        "[Kanagawa]": {
            "comments": "#8A8980",
        },
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "comment.block",
                    "comment.block.documentation",
                    "comment.line",
                    "comment.line.double-slash",
                    "punctuation.definition.comment",
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "workbench.startupEditor": "none",
    "window.openFilesInNewWindow": "off",
    "workbench.sideBar.location": "right",
    // Terminal
    "terminal.integrated.fontFamily": "'FiraCode Nerd Font', 'JetBrainsMono Nerd Font', 'monospace', monospace",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.tabs.enabled": false,
    //Explorer
    "explorer.confirmDelete": true,
    "explorer.compactFolders": false,
    "explorer.confirmDragAndDrop": false,
    //Emmet
    "emmet.triggerExpansionOnTab": true,
    // "emmet.showExpandedAbbreviation": "never",
    "emmet.includeLanguages": {
        "django-html": "html",
        "jinja-html": "html"
    },
    //Breadcrumbs Off
    "breadcrumbs.enabled": false,
    // Java/Typescript
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.preferences.quoteStyle": "double",
    "javascript.preferences.quoteStyle": "double",
    "javascript.format.semicolons": "insert",
    "typescript.format.semicolons": "insert",
    // "typescript.suggest.paths": false,
    // "javascript.suggest.paths": false,
    // Telemetry (VSCODE)
    "telemetry.telemetryLevel": "off",
    "vim.leader": "<space>",
    // "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.highlightedyank.enable": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        },
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>",
                "l",
                "a"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "s",
            ],
            "commands": [
                "editor.action.selectAll"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "t"
            ],
            "command": "workbench.view.extension.todo-tree-container"
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "-"
            ],
            "commands": [
                "vsnetrw.open"
            ],
            "when": "editorLangId != vsnetrw"
        },
        {
            "before": [
                "<leader>",
                "e"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "r"
            ],
            "commands": [
                ":registers"
            ]
        },
        {
            "before": [
                "<leader>",
                "q"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "<Esc>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "v"
            ],
            "commands": [
                "workbench.action.splitEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "<Enter>"
            ],
            "commands": [
                "workbench.action.splitEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "h"
            ],
            "commands": [
                "workbench.action.splitEditorDown"
            ]
        },
        // Harpoon
        {
            "before": [
                "<leader>",
                "s",
                "a"
            ],
            "commands": [
                "vscode-harpoon.addEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "s"
            ],
            "commands": [
                "vscode-harpoon.editEditors"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "p",
                "e"
            ],
            "commands": [
                "vscode-harpoon.editorQuickPick"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "j"
            ],
            "commands": [
                "vscode-harpoon.gotoEditor1"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "k"
            ],
            "commands": [
                "vscode-harpoon.gotoEditor2"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "l"
            ],
            "commands": [
                "vscode-harpoon.gotoEditor3"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                ";"
            ],
            "commands": [
                "vscode-harpoon.gotoEditor4"
            ]
        },
        {
            "before": [
                "<leader>",
                "-"
            ],
            "commands": [
                "workbench.action.splitEditorDown"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "o"
            ],
            "commands": [
                ":tabnew"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "x"
            ],
            "commands": [
                ":tabclose"
            ]
        },
        {
            "before": [
                "<S-h>"
            ],
            "commands": [
                ":tabp"
            ]
        },
        {
            "before": [
                "<S-l>"
            ],
            "commands": [
                ":tabnext"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "commands": [
                "workbench.action.showAllEditors"
            ]
        },
        {
            "before": [
                "<leader>",
                "g"
            ],
            "commands": [
                "workbench.action.findInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "r"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "a"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "<leader>",
                "a",
                "f"
            ],
            "commands": [
                "editor.action.autoFix"
            ]
        },
        {
            "before": [
                "<leader>",
                "m",
                "m"
            ],
            "commands": [
                "editor.action.format"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "a"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "<S-k>"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
        {
            "before": [
                "<leader>",
                "l"
            ],
            "commands": [
                "fuzzySearch.activeTextEditor"
            ]
        },
        // Diagnostics
        {
            "before": [
                "]",
                "e"
            ],
            "commands": [
                "editor.action.marker.next"
            ]
        },
        {
            "before": [
                "[",
                "e"
            ],
            "commands": [
                "editor.action.marker.prev"
            ]
        },
        {
            "before": [
                "<leader>",
                "o"
            ],
            "after": [
                "o",
                "<Esc>"
            ]
        },
        {
            "before": [
                "g",
                "g"
            ],
            "commands": [
                ":1"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
    ],
    "workbench.editor.showTabs": "single",
    "git.openRepositoryInParentFolders": "never",
    "todo-tree.highlights.customHighlight": {
        "WARN": {
            "foreground": "white",
            "background": "red",
            "iconColour": "red",
            "icon": "alert",
            "type": "text"
        },
        "TODO": {
            "foreground": "black",
            "background": "#98BB6C",
            "iconColour": "green",
            "icon": "check",
            "type": "text"
        },
        "FIXME": {
            "foreground": "black",
            "background": "#FFA066",
            "iconColour": "red",
            "icon": "bug"
        },
        "REVIEW": {
            "foreground": "black",
            "background": "cyan",
            "iconColour": "lightblue",
            "icon": "eye"
        },
        "HACK": {
            "foreground": "black",
            "background": "#FFA500",
            "iconColour": "orange",
            "icon": "alert"
        },
        "REF": {
            "foreground": "black",
            "background": "grey",
            "iconColour": "grey",
            "icon": "link",
            "type": "tag"
        }
    },
    "editor.stickyScroll.enabled": false,
    "window.menuBarVisibility": "toggle",
    "workbench.colorTheme": "Catppuccin Mocha"
}