{
    //Window
    "window.zoomLevel": 1,
    "window.density.editorTabHeight": "compact",
    //Editor
    "hide-tabs.toggleTabs": true,
    "editor.tabSize": 4,
    "editor.lineHeight": 17,
    "editor.fontLigatures": true,
    "editor.fontFamily": "'FiraCode Nerd Font', 'JetBrainsMono Nerd Font', 'monospace', monospace",
    "editor.wordWrap": "bounded",
    "editor.wrappingIndent": "same",
    "editor.lineNumbers": "relative",
    "editor.wordWrapColumn": 100,
    "editor.insertSpaces": true,
    "editor.smoothScrolling": false,
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "editor.detectIndentation": true,
    "editor.suggestSelection": "first",
    "editor.scrollBeyondLastLine": false,
    "editor.renderControlCharacters": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.quickSuggestionsDelay": 0,
    "editor.cursorBlinking": "solid",
    "editor.glyphMargin": true,
    "editor.bracketPairColorization.enabled": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.hideCursorInOverviewRuler": true,
    "workbench.sideBar.location": "right",
    "workbench.activityBar.location": "hidden",
    "workbench.editor.showTabs": "single",
    "keyboard.dispatch": "keyCode",
    "editor.unicodeHighlight.allowedCharacters": {
        "а": true,
        "с": true,
        "Т": true,
        "б": true,
        "е": true
    },
    "editor.formatOnSave": true,
    //Colorschemes - Themes
    "catppuccin.accentColor": "peach",
    "catppuccin.colorOverrides": {
        "mocha": {
            "pink": "#F5C5C2",
            "base": "#181825",
            "mauve": "#A8A6F7",
        }
    },
    "workbench.colorCustomizations": {
        "[Kanagawa]": {
            "editorBracketMatch.background": "#363646",
            "editorBracketMatch.border": "#7e9cd8",
            // "editor.background": "#161617"
            "terminal.background": "#1b1b22",
            "editorLineNumber.foreground": "#5E6982",
            "editorLineNumber.activeForeground": "#ED9338",
            "editor.background": "#191624",
            "activityBar.foreground": "#e0DEF4",
            "activityBarBadge.foreground": "#e0DEF4",
            "button.secondaryForeground": "#e0DEF4",
            "descriptionForeground": "#e0DEF4",
            "editor.foreground": "#e0DEF4",
            "editorCursor.foreground": "#957fb8",
            "list.activeSelectionForeground": "#e0DEF4",
            "list.focusForeground": "#e0DEF4",
            "list.hoverForeground": "#e0DEF4",
            "list.inactiveSelectionForeground": "#e0DEF4",
            "menu.foreground": "#e0DEF4",
            "settings.headerForeground": "#e0DEF4",
            "sideBar.foreground": "#e0DEF4",
            "sideBarSectionHeader.foreground": "#e0DEF4",
            "statusBar.debuggingForeground": "#e0DEF4",
            "statusBarItem.remoteForeground": "#e0DEF4",
            "tab.activeForeground": "#e0DEF4",
            "terminal.ansiBrightWhite": "#e0DEF4",
            "terminal.ansiWhite": "#e0DEF4",
            "terminal.foreground": "#e0DEF4",
            "titleBar.activeForeground": "#e0DEF4",
            "titleBar.inactiveForeground": "#e0DEF4",
        },
        "[nordic-vscode]": {
            "editorBracketMatch.background": "#363646",
            "editorBracketMatch.border": "#A3BE8C",
            // "editor.background": "#161617"
            "terminal.background": "#1B1F27",
            // "editorLineNumber.foreground": "#5E6982",
            "editorLineNumber.activeForeground": "#D08770",
            "editor.background": "#1B1F27",
            "editorError.foreground": "#f48771",
            "editorError.background": "#5e262c",
            "editorError.border": "#BF616A",
            "editorCursor.background": "#161617",
        },
        "[NeoRose-VimPine]": {
            "editorBracketHighlight.foreground2": "#3e8fb0",
            "terminal.ansiBlue": "#3e8fb0",
            "terminal.ansiBrightBlue": "#3e8fb0",
        },
    },
    "editor.semanticTokenColorCustomizations": {
        "[Kanagawa Black]": {
            "comments": "#8A8980",
            "background": "#161617"
        },
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "comment.block",
                    "comment.block.documentation",
                    "comment.line",
                    "comment.line.double-slash",
                    "punctuation.definition.comment",
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "editor.tokenColorCustomizations": {
        "[Kanagawa]": {
            "comments": "#8A8980",
        },
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "comment.block",
                    "comment.block.documentation",
                    "comment.line",
                    "comment.line.double-slash",
                    "punctuation.definition.comment",
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "workbench.startupEditor": "none",
    "window.openFilesInNewWindow": "off",
    // Terminal
    "terminal.integrated.fontFamily": "'FiraCode Nerd Font', 'JetBrainsMono Nerd Font', 'monospace', monospace",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.tabs.enabled": false,
    //Explorer
    "explorer.confirmDelete": true,
    "explorer.compactFolders": false,
    "explorer.confirmDragAndDrop": false,
    //Emmet
    "emmet.triggerExpansionOnTab": true,
    // "emmet.showExpandedAbbreviation": "never",
    "emmet.includeLanguages": {
        "django-html": "html",
        "jinja-html": "html"
    },
    // Java/Typescript
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.preferences.quoteStyle": "double",
    "javascript.preferences.quoteStyle": "double",
    "javascript.format.semicolons": "insert",
    "typescript.format.semicolons": "insert",
    // "typescript.suggest.paths": false,
    // "javascript.suggest.paths": false,
    // Telemetry (VSCODE)
    "telemetry.telemetryLevel": "off",
    "git.openRepositoryInParentFolders": "never",
    // TODO-TREE
    "todo-tree.highlights.customHighlight": {
        "WARN": {
            "foreground": "white",
            "background": "red",
            "iconColour": "red",
            "icon": "alert",
            "type": "text"
        },
        "TODO": {
            "foreground": "black",
            "background": "#98BB6C",
            "iconColour": "green",
            "icon": "check",
            "type": "text"
        },
        "FIXME": {
            "foreground": "black",
            "background": "#FFA066",
            "iconColour": "red",
            "icon": "bug"
        },
        "REVIEW": {
            "foreground": "black",
            "background": "cyan",
            "iconColour": "lightblue",
            "icon": "eye"
        },
        "HACK": {
            "foreground": "black",
            "background": "#FFA500",
            "iconColour": "orange",
            "icon": "alert"
        },
        "REF": {
            "foreground": "black",
            "background": "grey",
            "iconColour": "grey",
            "icon": "link",
            "type": "tag"
        }
    },
    "editor.stickyScroll.enabled": false,
    "window.menuBarVisibility": "toggle",
    "explorer.autoReveal": false,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "vscode-neovim.ctrlKeysForInsertMode": [
        "a",
        "c",
        "h",
        "j",
        "m",
        "o",
        "r",
        "t",
        "u",
        "w",
        "l"
    ],
    "vscode-neovim.compositeKeys": {
        "jj": {
            "command": "vscode-neovim.escape",
        },
        "jk": {
            // Use lua to execute any logic
            "command": "vscode-neovim.lua",
            "args": [
                [
                    "local api = vim.api",
                    "local mode = api.nvim_get_mode().mode",
                    "if mode == 'i' then",
                    "  api.nvim_input('<Esc>la')",
                    "end",
                ],
            ],
        },
    },
    "editor.showFoldingControls": "never",
    "workbench.editor.enablePreview": false,
    "editor.foldingImportsByDefault": false,
    "workbench.colorTheme": "Kanagawa",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-11",
            "path": "/home/daniel/Descargas/Programas/jdk-11.0.0.1",
            "default": true
        }
    ],
    "java.jdt.ls.java.home": "/home/daniel/Descargas/Programas/jdk-11.0.0.1"
}