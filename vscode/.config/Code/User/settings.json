{
  //Window
  "window.zoomLevel": 1,
  "window.density.editorTabHeight": "compact",
  //Editor
  "hide-tabs.toggleTabs": true,
  "editor.tabSize": 4,
  "editor.lineHeight": 14,
  "editor.fontLigatures": true,
  "editor.fontFamily": "'Hack Nerd Font','Iosevka Term','Liberation Mono','DejaVu Sans Mono Book','SauceCodePro NF','BlexMono Nerd Font Mono','JetBrainsMono Nerd Font',   'monospace', monospace",
  "editor.fontSize": 13,
  "editor.wordWrap": "bounded",
  "editor.wrappingIndent": "same",
  "editor.lineNumbers": "relative",
  "editor.wordWrapColumn": 100,
  "editor.insertSpaces": true,
  "editor.smoothScrolling": false,
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "editor.detectIndentation": true,
  "editor.suggestSelection": "first",
  "editor.scrollBeyondLastLine": false,
  "editor.renderControlCharacters": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.quickSuggestionsDelay": 0,
  "editor.cursorBlinking": "solid",
  "editor.glyphMargin": true,
  "editor.bracketPairColorization.enabled": false,
  "editor.scrollbar.horizontal": "visible",
  "editor.scrollbar.vertical": "visible",
  "editor.hideCursorInOverviewRuler": true,
  "workbench.editor.showTabs": "single",
  "keyboard.dispatch": "keyCode",
  "editor.unicodeHighlight.allowedCharacters": {
    "а": true,
    "с": true,
    "Т": true,
    "б": true,
    "е": true
  },
  "editor.formatOnSave": true,
  //Colorschemes - Themes
  "catppuccin.accentColor": "peach",
  "catppuccin.colorOverrides": {
    "mocha": {
      "pink": "#F5C5C2",
      "base": "#181825",
      "mauve": "#A8A6F7"
    }
  },
  "workbench.colorCustomizations": {
    "[Kanagawa]": {
      "editorBracketMatch.background": "#363646",
      "editorBracketMatch.border": "#7e9cd8",
      "editor.background": "#222222",
      "sideBar.background": "#343434",
      "terminal.background": "#121216",
      "editorLineNumber.foreground": "#848ea7",
      "editorLineNumber.activeForeground": "#EDB397",
      "activityBar.foreground": "#e0DEF4",
      "badge.foreground": "#e0DEF4",
      "dropdown.foreground": "#e0DEF4",
      "activityBarBadge.background": "#e0DEF4",
      "editorHoverWidget.highlightForeground": "#e0DEF4",
      "activityBarBadge.foreground": "#e0DEF4",
      "button.secondaryForeground": "#e0DEF4",
      "descriptionForeground": "#e0DEF4",
      "editor.foreground": "#e0DEF4",
      "symbolIcon.variableForeground": "#e0DEF4",
      "editorCursor.foreground": "#957fb8",
      "list.activeSelectionForeground": "#e0DEF4",
      "list.focusForeground": "#e0DEF4",
      "list.hoverForeground": "#e0DEF4",
      "list.inactiveSelectionForeground": "#e0DEF4",
      "menu.foreground": "#e0DEF4",
      "menu.background": "#16161d",
      "selection.background": "#275351",
      "editor.selectionBackground": "#275351",
      "editor.selectionHighlightBackground": "#275351",
      "statusBar.foreground": "#e5e9f0",
      "editorSuggestWidget.background": "#191921",
      "editorSuggestWidget.selectedBackground": "#464C8B",
      "settings.headerForeground": "#e0DEF4",
      "sideBar.foreground": "#e0DEF4",
      "sideBarSectionHeader.foreground": "#e0DEF4",
      "statusBar.debuggingForeground": "#e0DEF4",
      "statusBarItem.remoteForeground": "#e0DEF4",
      "tab.activeForeground": "#e0DEF4",
      "terminal.ansiBrightWhite": "#e0DEF4",
      "terminal.ansiWhite": "#e0DEF4",
      "terminal.foreground": "#e0DEF4",
      "titleBar.activeForeground": "#e0DEF4",
      "titleBar.inactiveForeground": "#e0DEF4",
      "search.resultsInfoForeground": "#e0DEF4"
    }
  },
  "editor.tokenColorCustomizations": {
    "[Kanagawa]": {
      "comments": "#8A8980",
      "functions": "#9EBCFF",
      "variables": "#e0def4",
      "strings": "#AFD185",
      "keywords": "#AB92D3",
      "types": "#AB92D3",
      "textMateRules": [
        {
          "scope": [
            "comment",
            "string",
            "comment.block",
            "comment.block.documentation",
            "comment.line",
            "comment.line.double-slash",
            "punctuation.definition.comment"
          ],
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "constant",
            "keyword"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "name": "Variables",
          "scope": [
            "variable",
            "string constant.other.placeholder"
          ],
          "settings": {
            "foreground": "#e0DEF4"
          }
        },
        {
          "name": "Other Variable",
          "scope": [
            "support.other.variable"
          ],
          "settings": {
            "foreground": "#e0DEF4"
          }
        },
        {
          "name": "meta.method.js",
          "scope": [
            "meta.class-method.js entity.name.function.js",
            "variable.function.constructor",
            "entity.name.method.js"
          ],
          "settings": {
            "foreground": "#9EBCFF"
          }
        },
        {
          "scope": "entity.name.function",
          "settings": {
            "foreground": "#9EBCFF"
          }
        },
        {
          "scope": "string.quoted",
          "settings": {
            "foreground": "#AFD185"
          }
        }
      ]
    }
  },
  "workbench.startupEditor": "none",
  "window.openFilesInNewWindow": "off",
  // Terminal
  "terminal.integrated.fontFamily": "'FiraCode Nerd Font', 'JetBrainsMono Nerd Font', 'monospace', monospace",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.tabs.enabled": false,
  "terminal.integrated.profiles.osx": {
    //...existing profiles...
    "tmux-shell": {
      "path": "tmux",
      "args": [
        "new-session",
        "-A",
        "-s",
        "vscode:${workspaceFolder}"
      ]
    }
  },
  "terminal.integrated.defaultProfile.osx": "tmux-shell",
  //Explorer
  "explorer.confirmDelete": true,
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  //Emmet
  "emmet.triggerExpansionOnTab": true,
  // "emmet.showExpandedAbbreviation": "never",
  "emmet.includeLanguages": {
    "django-html": "html",
    "jinja-html": "html"
  },
  // Java/Typescript
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.quoteStyle": "double",
  "javascript.preferences.quoteStyle": "double",
  "javascript.format.semicolons": "insert",
  "typescript.format.semicolons": "insert",
  // "typescript.suggest.paths": false,
  // "javascript.suggest.paths": false,
  // Telemetry (VSCODE)
  "telemetry.telemetryLevel": "off",
  "git.openRepositoryInParentFolders": "never",
  // TODO-TREE
  "todo-tree.highlights.customHighlight": {
    "WARN": {
      "foreground": "white",
      "background": "red",
      "iconColour": "red",
      "icon": "alert",
      "type": "text"
    },
    "TODO": {
      "foreground": "black",
      "background": "#98BB6C",
      "iconColour": "green",
      "icon": "check",
      "type": "text"
    },
    "FIXME": {
      "foreground": "black",
      "background": "#FFA066",
      "iconColour": "red",
      "icon": "bug"
    },
    "REVIEW": {
      "foreground": "black",
      "background": "cyan",
      "iconColour": "lightblue",
      "icon": "eye"
    },
    "HACK": {
      "foreground": "black",
      "background": "#FFA500",
      "iconColour": "orange",
      "icon": "alert"
    },
    "REF": {
      "foreground": "black",
      "background": "grey",
      "iconColour": "grey",
      "icon": "link",
      "type": "tag"
    }
  },
  "editor.stickyScroll.enabled": false,
  "explorer.autoReveal": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "vscode-neovim.ctrlKeysForInsertMode": [
    "a",
    "c",
    "h",
    "j",
    "m",
    "o",
    "r",
    "t",
    "u",
    "w",
    "l"
  ],
  "vscode-neovim.compositeKeys": {
    "jk": {
      "command": "vscode-neovim.escape"
    }
  },
  "editor.showFoldingControls": "never",
  "workbench.editor.enablePreview": false,
  "editor.foldingImportsByDefault": false,
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-11",
      "path": "/home/daniel/Descargas/Programas/jdk-11.0.0.1",
      "default": true
    }
  ],
  "java.jdt.ls.java.home": "/home/daniel/Descargas/Programas/jdk-11.0.0.1",
  "material-icon-theme.folders.color": "#7e9cd8",
  "material-icon-theme.hidesExplorerArrows": false,
  "material-icon-theme.folders.theme": "specific",
  "window.titleBarStyle": "native",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "workbench.colorTheme": "Kanagawa",
  "telemetry.feedback.enabled": false,
  "python.analysis.extraPaths": [
    "./src"
  ],
  "workbench.sideBar.location": "left",
  "window.customTitleBarVisibility": "never",
  "aws.telemetry": false,
  "window.menuBarVisibility": "toggle",
  "workbench.activityBar.location": "hidden",
  "jupyter.askForKernelRestart": false
}